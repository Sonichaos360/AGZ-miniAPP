<?xml version='1.0' encoding='utf-8'?>
<widget id="com.emeraldpower.agzradioapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AGZ Radio!</name>
    <description>
        Aplicaci√≥n oficial de AGZRADIO.COM
    </description>
    <author email="contacto@lucianovergara.com" href="http://agzradio.com">
        Luciano Vergara
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="img/logo.png" />
</widget>

